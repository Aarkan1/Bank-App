<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="25.0" prefWidth="300.0" styleClass="transactionPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.transaction.TransactionController">
   <children>
      <HBox layoutX="2.0" layoutY="4.0">
         <children>
            <Label fx:id="date" prefHeight="17.0" prefWidth="83.0" text="Date">
               <font>
                  <Font name="Lucida Sans Regular" size="12.0" />
               </font></Label>
            <Label fx:id="message" prefHeight="17.0" prefWidth="130.0" text="Message">
               <font>
                  <Font name="Lucida Sans Regular" size="12.0" />
               </font></Label>
            <Label fx:id="amount" alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="83.0" text="Amount">
               <font>
                  <Font name="Lucida Sans Regular" size="12.0" />
               </font></Label>
         </children>
      </HBox>
   </children>
</AnchorPane>
